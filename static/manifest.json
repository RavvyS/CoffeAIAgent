  "screenshots": [
    {
      "src": "/static/screenshots/mobile-chat.png",
      "sizes": "375x812",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "/static/screenshots/desktop-dashboard.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide"
    }
  ],
  "categories": ["food", "productivity", "social"],
  "shortcuts": [
    {
      "name": "Start New Order",
      "short_name": "New Order",
      "description": "Start a new coffee order",
      "url": "/?action=order",
      "icons": [
        {
          "src": "/static/icons/shortcut-order.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "View Menu",
      "short_name": "Menu",
      "description": "Browse our coffee menu",
      "url": "/?action=menu",
      "icons": [
        {
          "src": "/static/icons/shortcut-menu.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Analytics Dashboard",
      "short_name": "Analytics",
      "description": "View business analytics",
      "url": "/analytics/dashboard",
      "icons": [
        {
          "src": "/static/icons/shortcut-analytics.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+coffeeshop",
      "url": "/?order=%s"
    }
  ]
}